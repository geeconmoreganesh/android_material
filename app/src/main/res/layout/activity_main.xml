<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="80dp"
    android:background="@drawable/curve_layout"
    android:orientation="horizontal">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="20dp"
        android:src="@drawable/ic_baseline_arrow_back_ios_24"
        app:tint="#ffffff"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Title"
        android:layout_gravity="center"
        android:textSize="25dp"
        android:textColor="#fff"
        android:layout_marginLeft="150dp"/>
</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_YoutubeVideo"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="You tube Video"/>
        <TextView
            android:id="@+id/Looping_Statement"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="Looping Statement"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_varabale"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="Master Varable"/>
        <TextView
            android:id="@+id/tv_master_arrey"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="Master array"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_calculater"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="example"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="example2"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/for_loop"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:fontFamily="serif"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="example2"/>

        <TextView
            android:id="@+id/api"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:fontFamily="serif"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="api"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/android_ui_control"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:fontFamily="serif"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="android Widgets"/>

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="15sp"
            android:padding="10dp"
            android:fontFamily="serif"
            android:textColor="@color/grey_800"
            android:layout_height="wrap_content"
            android:text="api"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
      <ImageView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          android:src="@mipmap/ic_launcher_round"></ImageView>
    </LinearLayout>

</LinearLayout>